[plugin]
plugin=amazon.aws.aws_ec2
aws_profile=default

[regions]
regions=us-east-1

[strict]
strict=False

[keyed_groups]
keyed_groups=tags
prefix=tag

[filters]
tag:Name=DIO_bastion
tag:Name=DIO_nginx
tag:Name=DIO_tooling
tag:Name=DIO_wordpress
instance-state-name=running

[hostnames]
hostnames=private-ip-address
hostnames=tag:Name
hostnames=dns-name
hostnames=ip-address

[compose]
ansible_host=private_ip_address
